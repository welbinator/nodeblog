<%- include ("partials/header.ejs") %>

<h1>Index Page</h1>

<% blogs.forEach((blog) => { %>
<div>
    <h2><%= blog.title %></h2>
    <img src="<%= blog.image %>">
    <p><%= blog.body %></p>
    <a href="/blogs/<%= blog._id %>">Read More</a>
    <p>Created on: <%= blog.date %></p>
</div>

<% }); %>


<%- include ("partials/footer.ejs") %>